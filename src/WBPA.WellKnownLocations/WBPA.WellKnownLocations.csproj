<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
    <SccProjectName>SAK</SccProjectName>
    <SccProvider>SAK</SccProvider>
    <SccAuxPath>SAK</SccAuxPath>
    <SccLocalPath>SAK</SccLocalPath>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.0.2018.840</Version>
    <Authors>Michael Mortensen</Authors>
    <Company>Weubphoria</Company>
    <PackageLicenseUrl>https://github.com/wbpa/well-known/blob/master/LICENSE.md</PackageLicenseUrl>
    <Product>Well-Known Locations by Weubphoria</Product>
    <Description>The WBPA.WellKnownLocations assembly provides a way for ASP.NET Core applications to open up the RFC 5785 .well-known directory for sitewide metadata files.</Description>
    <Copyright>Copyright Â© Weubphoria 2009-2018. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/wbpa/well-known</PackageProjectUrl>
    <PackageIconUrl>https://nblcdn.net/themes/weubphoria.dk/nuget/wbpa-rfc-5785.png</PackageIconUrl>
    <PackageTags>wellknown wellknownlocation wellknownlocations well-known location locations acme-challenge assetlinks.json asp.net core</PackageTags>
    <AssemblyVersion>1.0.2018.840</AssemblyVersion>
    <FileVersion>1.0.2018.840</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cuemon.Core" Version="4.2.2018.840" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="1.1.3" />
    <PackageReference Include="Microsoft.Extensions.FileProviders.Physical" Version="1.1.1" />
  </ItemGroup>

</Project>